{
  "name": "Order Router",
  "scope": "ponomarevlad",
  "builds": [
    {
      "src": "index.php",
      "use": "@now/php"
    },
    {
      "src": "*.js",
      "use": "@now/static"
    }
  ],
  "regions": [
    "cdg1"
  ],
  "public": false,
  "env": {
    "iftttToken": "@ifttt-token",
    "mailLogin": "@mail-login",
    "mailPass": "@mail-pass"
  }
}